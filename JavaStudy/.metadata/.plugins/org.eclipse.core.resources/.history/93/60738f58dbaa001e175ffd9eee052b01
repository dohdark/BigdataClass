package javaFestival;

import java.util.Scanner;

public class test {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.print("정수 입력 : ");
		int num = sc.nextInt();
		int sum = 0;
		int count=1;
		int result = 0;
		// 25462581
		
		for(int i = 1 ; i <= 8 ; i ++) {
			result = num/count^i;
		}
		
		
		
		System.out.println("합은 : "+result);
		
		
		
		
		
		
	}

}
