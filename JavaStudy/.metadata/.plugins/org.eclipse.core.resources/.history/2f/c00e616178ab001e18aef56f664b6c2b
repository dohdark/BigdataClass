package 이중for문;

public class Ex03공포의별찍기 {

	public static void main(String[] args) {

		int fk=4;
		for (int i = 0; i <= fk; i++) {
			for(int j = 0; j <= fk-i; j++) {
				System.out.print("1");
			}
			for(int k = 0; k <= fk-j; k++) {
				System.out.print("2");
			}
			System.out.println("");
		}
		
		
		
		
		
		
		
	}

}
