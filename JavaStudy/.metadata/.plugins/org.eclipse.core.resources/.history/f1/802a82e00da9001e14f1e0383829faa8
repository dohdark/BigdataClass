package 다중if문;

import java.util.Scanner;

public class Ex_02_학점계산프로그램 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("점수를 입력하세요 : ");
		int score = sc.nextInt();
		String a="";
		if(score>=90) {
			a="A";
		}else if(score>=80) {
			a="B";
		}else if(score>=70) {
			a="C";
		}else
			a="D";
		System.out.println(a+" 학점입니다.");
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
