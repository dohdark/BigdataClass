package 배열;

import java.util.Scanner;

public class Ex05점수계산 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);		
		
		int[] array = new int[5];
		
		int max, min = 0;
		
		for(int i = 0; i<=4;i++) {
			System.out.println("0~4번 인덱스에 들어갈 정수 입력 : ");
			array[i]=sc.nextInt();
		}
		
		
		
	}

}
