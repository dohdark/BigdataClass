package javaFestival;

import java.util.Scanner;

public class javaFestival_04 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.print("첫 번째 숫자입력 : ");
		int firstNum=sc.nextInt();
		System.out.print("두 번째 숫자입력 : ");
		int secondNum=sc.nextInt();
		
		
		System.out.println((secondNum%10)*firstNum);
		System.out.println(((secondNum%100 - secondNum%10))*firstNum);
		System.out.println((secondNum/100)*firstNum);
		System.out.println(secondNum*firstNum);
		
		
	}
}
