package javaFestival;

public class javaFestival_04일_03문제 {

	public static void main(String[] args) {
												
		for(int i = 4; i >= 0 ; i--) {		 // 0 1 2 3 4
			System.out.print("n");
			System.out.println("");
			for(int j = i; j <= i-j ; j++) { // 0 1 2 3 4
				System.out.print("*");
			}
		}
	}

}
