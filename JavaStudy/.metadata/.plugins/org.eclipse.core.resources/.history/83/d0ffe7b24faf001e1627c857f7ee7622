package 이차원배열;

public class Ex01이차원배열생성 {

	public static void main(String[] args) {

		int[][] array = new int[3][3];
		array[1][1]=5;
		System.out.println(array[1][1]);
		
		//이차원배열에 값 넣기(1~9)
		for(int i = 0; i <=2 ; i++) {
			for(int j = 0; j<=2; j++) {
				array[i][j]=j;
				System.out.println(array[i][j]);
			}
		}
		
		
		
	}

}
