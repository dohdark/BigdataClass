package 이중for문;

public class Ex03공포의별찍기 {

	public static void main(String[] args) {

		int fk=4;
		for (int i = 0; i <= fk; i++) {
			System.out.println("1");
			for(int j = 0; j <= fk; j++) {
				System.out.println("2");
			}
			for(int k = 0; k <= fk-i; k++) {
				System.out.println("3");
			}
		}
		
		
		
		
		
		
		
	}

}
