package javaFestival;

import java.util.Scanner;

public class javaFestival_03일_04문제 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("n 입력: ");
		int n = sc.nextInt();
		int sum = 0;
		int count = 1;
		
		
		for(int i = 1 ; i< n ; i++) {
			sum += count;
			count++;
			System.out.println(count+i);
		}
		
		
		
	}

}
