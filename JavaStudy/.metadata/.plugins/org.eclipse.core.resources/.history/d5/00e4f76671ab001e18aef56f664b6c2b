package javaFestival;

public class javaFestival_04일_03문제 {

	public static void main(String[] args) {
											 //    
		for(int i = 4; i >= 0 ; i--) {		 // i= 4 3 2 1 0
			System.out.print("ln\n");
			for(int j = i; j <= 4 ; j++) { 	 // j= 0 1 2 3 4
				
				System.out.print("*");
			}
		}
	}

}
